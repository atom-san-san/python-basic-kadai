        "# グローバルスコープ\n",
        "price1 = 100\n",
        "price2 = 200\n",
        "\n",
        "def total_with_tax():\n",
        "    # ローカルスコープ\n",
        "    tax = 1.1\n",
        "    total_price = price1 + price2\n",
        "    return total_price * tax\n",
        "\n",
        "print(total_with_tax())\n"
