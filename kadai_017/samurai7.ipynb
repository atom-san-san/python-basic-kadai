        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(self.name)\n",
        "        print(self.age)\n",
        "\n",
        "\n",
        "# Humanクラスのインスタンスを変数に代入\n",
        "person = Human(\"Taro\", 20)\n",
        "\n",
        "# メソッドを使用して名前と年齢を出力\n",
        "person.printinfo()\n"
      ]
    }
