      "source": [
        "# 台形の各辺の長さ（cm）\n",
        "upper_base = 10   # 上辺\n",
        "lower_base = 20   # 下辺\n",
        "height = 5        # 高さ\n",
        "\n",
        "# 台形の面積の計算\n",
        "area = (upper_base + lower_base) * height / 2\n",
        "\n",
        "# 結果の表示\n",
        "print(f\"台形の面積は {area} cm²です。\")"
      ]
